{
  "name": "First",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "link-assets": "react-native-asset",
    "reverse": "adb start-server && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:9090 tcp:9090",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "pods": "run-script-os",
    "pods:macos": "npx pod-install",
    "pods:default": "echo '\\033[0;33m\"pods\" command available on mac only!'",
    "android:dev": "react-native run-android --mode=developmentdebug",
    "android:staging": "react-native run-android --mode=stagingdebug",
    "android:prod": "react-native run-android --mode=productiondebug",
    "android:dev-release": "react-native run-android --mode=developmentrelease",
    "android:staging-release": "react-native run-android --mode=stagingrelease",
    "android:prod-release": "react-native run-android --mode=productionrelease",
    "apk:dev": "cd android && ./gradlew assembleDevelopment && cd ..",
    "apk:staging": "cd android && ./gradlew assembleStaging && cd ..",
    "apk:prod": "cd android && ./gradlew assembleProduction && cd ..",
    "bundle:dev": "cd android && ./gradlew bundleDevelopment && cd ..",
    "bundle:staging": "cd android && ./gradlew bundleStaging && cd ..",
    "bundle:prod": "cd android && ./gradlew bundleProduction && cd ..",
    "ios-clean": "run-script-os",
    "ios-clean:macos": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "ios-clean:default": "echo '\\033[0;33m\"ios-clean\" command available on mac only!'",
    "ios:dev": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Debug\"",
    "ios:dev-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Debug\"",
    "ios:dev-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Debug\"",
    "ios:dev-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Debug\"",
    "ios:staging": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Staging\"",
    "ios:staging-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Staging\"",
    "ios:staging-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Staging\"",
    "ios:staging-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Staging\"",
    "ios:prod": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Release\"",
    "ios:prod-large": "react-native run-ios --simulator=\"iPhone 13 Pro Max\" --scheme \"Release\"",
    "ios:prod-small": "react-native run-ios --simulator=\"iPhone 8 Plus\" --scheme \"Release\"",
    "ios:prod-ipad": "react-native run-ios --simulator=\"iPad Pro (12.9-inch) (6th generation)\" --scheme \"Release\"",
    "ios:dev-release": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Debug\" --configuration Release",
    "ios:staging-release": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Staging\" --configuration Release",
    "ios:prod-release": "react-native run-ios --simulator=\"iPhone 14 Pro Max\" --scheme \"Release\" --configuration Release",
    "start": "react-native start",
    "start-reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "husky install",
    "postinstall": "patch-package && yarn pods && yarn link-assets && yarn android-clean && yarn ios-clean",
    "ver:test": "yarn version --prerelease --preid alpha && react-native-version --amend --never-increment-build",
    "ver": "yarn version && react-native-version --amend --reset-build",
    "svg": "react-native-vector-image generate",
    "reset": "react-native clean-project-auto && yarn"
  },
  "dependencies": {
    "@eslam-elmeniawy/react-native-common-components": "^1.2.4",
    "@klarna/react-native-vector-drawable": "^0.5.0",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/analytics": "^18.8.0",
    "@react-native-firebase/app": "^18.8.0",
    "@react-native-firebase/crashlytics": "^18.8.0",
    "@react-native-firebase/messaging": "^18.8.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/material-bottom-tabs": "^6.2.22",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.1.0",
    "@tanstack/react-query": "^5.18.1",
    "axios": "^1.6.7",
    "i18next": "^23.8.2",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.50.0",
    "react-i18next": "^14.0.1",
    "react-native": "0.73.3",
    "react-native-bars": "^2.4.3",
    "react-native-bootsplash": "^5.3.0",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.12.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-image-slider-box": "^2.0.7",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localize": "^3.0.6",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-orientation-locker": "^1.6.0",
    "react-native-paper": "^5.12.3",
    "react-native-progress": "^5.0.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^14.1.0",
    "react-native-toast-notifications": "^3.4.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-vector-image": "^0.4.5",
    "react-redux": "^9.1.0",
    "redux-logger": "^3.0.6",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/runtime": "^7.23.9",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@evilmartians/lefthook": "^1.6.1",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.4",
    "@react-native/typescript-config": "0.73.1",
    "@rnx-kit/align-deps": "^2.3.3",
    "@tanstack/eslint-plugin-query": "^5.18.1",
    "@types/i18n-js": "^3.8.9",
    "@types/react": "^18.2.52",
    "@types/react-native-push-notification": "^8.1.4",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-logger": "^3.0.13",
    "@types/tinycolor2": "^1.4.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-ft-flow": "^3.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-func": "^0.1.18",
    "jest": "^29.7.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.4",
    "react-native-asset": "^2.1.1",
    "react-native-clean-project": "^4.0.3",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "5.0.3",
    "reactotron-react-query": "^1.0.2",
    "run-script-os": "^1.1.6",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.73"
      ],
      "capabilities": [
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "core/metro-config",
        "jest",
        "react",
        "react-test-renderer"
      ]
    }
  }
}
